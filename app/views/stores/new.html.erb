<center>
<h3>New Store</h3>
</center>

<script>$(document).ready(function(){
    $('select').formSelect();
  });</script>


<div class="container">

<%= form_for @store do |f| %>
<%= f.label "Name:" %> <br>
<%= f.text_field :name %>
<br><br>

<%= f.label "Password:" %> <br>
<%= f.password_field :password %>
<br><br>


<%= f.submit "Create Store Account" %>

<% end %>

</div>
