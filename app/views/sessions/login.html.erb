<center>
<h3>Log In</h3>
</center>

<script>$(document).ready(function(){
    $('select').formSelect();
  });</script>
<div class="container">

<%= form_tag login_path do %>
    <div class="input-field">
        <%= label_tag "Store Name:" %> <br>
        <%= text_field_tag :name%>
    </div>
    <div class="input-field">
        <%= label_tag :password %> <br>
        <%= password_field_tag :password %>
    </div>
    <%= submit_tag "Login" %>
<% end %>

</div>
