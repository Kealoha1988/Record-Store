
<h1>Log In</h1>

<%= form_tag login_path do %>
    <div class="input-field">
        <%= label_tag "Store Name:" %> <br>
        <%= text_field_tag :name%>
    </div>
    <div class="input-field">
        <%= label_tag :password %> <br>
        <%= password_field_tag :password %>
    </div>
    <%= submit_tag "Login" %>
<% end %>